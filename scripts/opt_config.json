{
  "optimizer": {
    "class_name": "CMAES",
    "config": {
      "pop_size": 25,
      "max_iter": 60,
      "verbose": -10
    }
  },
  "search_space": {
    "data": [
      {
        "class_name": "YahooFinanceCSVData",
        "config": {
          "dataname": "/work/algotrade/data/SPY_2015-01-02_2020-04-30.csv",
          "timeframe": 5
        }
      }
    ],
    "broker": {
      "class_name": "BacktestBroker",
      "config": {
        "cash": 1000000.0
      }
    },
    "strategy": [
      {
        "class_name": "LongShortStrategy",
        "params": {
          "use_ema": {
            "class_name": "BooleanParameterNode",
            "config": {
              "label": "use_ema"
            }
          },
          "fast_period": {
            "class_name": "IntParameterNode",
            "config": {
              "label": "fast_period",
              "min_val": 5,
              "max_val": 15
            }
          },
          "slow_period": {
            "class_name": "IntParameterNode",
            "config": {
              "label": "slow_period",
              "min_val": 25,
              "max_val": 50
            }
          },
          "stop_loss": {
            "class_name": "FloatParameterNode",
            "config": {
              "label": "stop_loss",
              "min_val": 0.001,
              "max_val": 0.5
            }
          },
          "trail": {
            "class_name": "BooleanParameterNode",
            "config": {
              "label": "trail"
            }
          },
          "onlylong": {
            "class_name": "BooleanParameterNode",
            "config": {
              "label": "onlylong"
            }
          }
        }
      }
    ],
    "sizers": [
      {
        "class_name": "FixedSize",
        "params": {
          "stake": {
            "class_name": "IntParameterNode",
            "config": {
              "label": "stake",
              "min_val": 1,
              "max_val": 100
            }
          }
        }
      }
    ],
    "analyzers": [
      {
        "class_name": "VWR",
        "params": {
          "_name": "ret"
        }
      }
    ]
  }
}